{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:watch": "vitest -u",
    "test:unit:coverage": "npx vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "HEADLESS=false playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "HEADLESS=false playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:build:cache": "synpress --headless",
    "test:build:cache:force": "synpress --headless --force",
    "test:headless": "HEADLESS=true playwright test",
    "coverage": "npx serve coverage/",
    "prod": "node dist/index.js",
    "build-only": "vite build",
    "type-check": "vue-tsc --build tsconfig.app.json --force",
    "lint": "eslint . --fix ",
    "format": "prettier --write .",
    "format-check": "prettier . --check",
    "sentry:releases": "sentry-cli releases new \"$(git rev-parse --short HEAD)\" --project cnc-portal-app && sentry-cli releases set-commits \"$(git rev-parse --short HEAD)\" --auto --project cnc-portal-app",
    "git:ignore-locally": "git update-index --assume-unchanged src/artifacts/deployed_addresses/chain-31337.json"
  },
  "dependencies": {
    "@apollo/client": "^3.13.1",
