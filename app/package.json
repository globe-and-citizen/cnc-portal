{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:watch": "vitest -u",
    "test:unit:coverage": "npx vitest run --coverage",
    "test": "playwright test",
    "test:debug": "playwright test --debug",
    "test:build:cache": "synpress",
    "test:headless": "HEADLESS=true playwright test",
    "coverage": "npx serve coverage/",
    "prod": "node  dist/index.js",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write .",
    "format-check": "prettier . --check"
  },
  "dependencies": {
    "@heroicons/vue": "^2.1.4",
    "@rollup/plugin-inject": "^5.0.5",
    "@tanstack/vue-query": "^5.54.1",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/core": "^10.9.0",
    "@wagmi/vue": "^0.0.40",
    "buffer": "^6.0.3",
    "dayjs": "^1.11.10",
    "echarts": "^5.5.1",
    "ethers": "^6.13.1",
    "pinia": "^2.1.7",
    "siwe": "^2.1.4",
    "viem": "^2.21.2",
    "vue": "^3.4.15",
    "vue-echarts": "^7.0.0-beta.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@playwright/test": "^1.44.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@synthetixio/synpress": "^4.0.0-alpha.9",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-istanbul": "^2.0.4",
    "@vitest/ui": "^2.0.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.17",
    "daisyui": "^4.7.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.35",
    "prettier": "^3.0.3",
    "sass": "^1.75.0",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vitest": "^2.0.4",
    "vue-tsc": "^1.8.27"
  }
}
